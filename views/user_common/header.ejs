    <style>
        ::-webkit-scrollbar {
            width: 0;
        }

        .main_content {
            background: linear-gradient(90deg, #aa076b, #61045f);
            background-attachment: fixed;
        }

        .bg-custom-1 {
            background: linear-gradient(90deg, #aa076b, #61045f) !important;
        }

        /* .banner { */
        /* background: url("img/1.jpg"); */
