        .election_header {
            font-size: calc(5vh + 5vw);
        }


        .media {
            min-height: 150px;
        }

        #time .h3 {
            font-size: calc(4vh + 4vw);
        }

        .move-cont {
            position: relative;
            bottom: 0;
            margin: 0 auto;
        }

        .move {
            position: relative;
            transition: 3s;
            animation: updown 2s infinite;
        }

        @keyframes updown {

            0%,
            20%,
            50%,
            80%,
            100% {
                transform: translateY(0);
            }

            40% {
                transform: translateY(-30px);
            }

            60% {
                transform: translateY(-15px);
            }
        }

        .symbolWrapper {
            height: 100px;
            width: 100px;
            border-radius: 50%;
            overflow: hidden;
            /* border: .1rem solid #333; */
            display: inline-block;
        }
    </style>
</head>


